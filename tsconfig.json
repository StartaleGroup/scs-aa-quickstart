{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "scs-smart-account-sdk": ["./node_modules/scs-smart-account-sdk/dist/_types"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/scs-smart-account-sdk/dist/_types",
      "./src/types"
    ]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
