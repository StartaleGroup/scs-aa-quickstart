{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@biconomy/abstractjs": ["../livingrock-abstractjs-fork/dist/_types"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "../livingrock-abstractjs-fork/dist/_types"
    ]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
